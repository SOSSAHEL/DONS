{
    "name": "ecedi/ecollecte",
    "license": "MIT",
    "type": "project",
    "description": "eCollecte standard edition, by Agence Ecedi http://ecedi.fr",
    "autoload": {
        "psr-0": { "": "src/" }
    },
    "require": {
        "php": ">=5.3.3",
        "symfony/symfony": "2.3.*",
        "doctrine/orm": ">=2.2.3,<2.4-dev",
        "doctrine/doctrine-bundle": "1.2.*",
        "twig/extensions": "1.0.*",
        "symfony/assetic-bundle": "2.3.*",
        "symfony/swiftmailer-bundle": "2.3.*",
        "symfony/monolog-bundle": "2.3.*",
        "sensio/framework-extra-bundle": "2.3.*",
        "incenteev/composer-parameter-handler": "~2.0",
        "friendsofsymfony/user-bundle": "~2.0@dev",
        "knplabs/knp-paginator-bundle": "2.3.*",
        "knplabs/knp-menu":"dev-master",
        "knplabs/knp-menu-bundle":"dev-master",
        "leafo/lessphp": "0.4.0",
        "liip/theme-bundle" : "dev-master",
        "liip/imagine-bundle" : "dev-master",
        "jms/serializer-bundle" : "0.13.*",
        "stof/doctrine-extensions-bundle" : "~1.1@dev",
        "friendsofsymfony/rest-bundle": "1.2.*",
        "doctrine/doctrine-fixtures-bundle": "dev-master",
        "jms/translation-bundle": "dev-master",
        "cnerta/breadcrumb-bundle": "1.0.*",
        "friendsofsymfony/oauth-server-bundle": "dev-master",
        "knplabs/knp-markdown-bundle": "1.2.*@dev",
        "vich/uploader-bundle": "dev-master",
        "knplabs/knp-gaufrette-bundle" : "dev-master",
        "friendsofsymfony/jsrouting-bundle": "@stable",
        "sp/bower-bundle": "dev-master",
        "trsteel/ckeditor-bundle": "~1.4",
        "helios-ag/fm-elfinder-bundle": "1.x",
        "campaignmonitor/createsend-php": "4.*",
        "zendframework/zendpdf": "2.*"
    },
    "require-dev": {
        "sensio/generator-bundle": "2.3.*",
        "raulfraile/ladybug-bundle": "dev-master",
        "phpspec/phpspec": "2.0.*@dev",
        "sensio/distribution-bundle": "2.3.*",
        "phpmd/phpmd" : "@stable" ,
        "henrikbjorn/phpspec-code-coverage": "~0.2"
    },
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sp\\BowerBundle\\Composer\\ScriptHandler::bowerInstall"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sp\\BowerBundle\\Composer\\ScriptHandler::bowerInstall"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "symfony-assets-install": "symlink",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        },
        "branch-alias": {
            "dev-master": "2.3-dev"
        }
    }
}
